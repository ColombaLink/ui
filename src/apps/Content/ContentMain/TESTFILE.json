// Viewtje

{
    "type": "content",
    "view": "table",
    "function": {
      "name": "db",
      "type": "query",
      "payload": {
        "$id": "10bdde6be6",
        "children": {
          "$list": true,
          "title": true,
          "id": true
        },
        "$all": true
      },
      "props": {
        "data": "$data.children",
        "onClick": {
          "view": {
            "type": "content-modal",
            "function": {
              "payload": {
                "$id": "$item.id",
                "title": true,
                "id": true
              }
            }
          }
        },
        "fields": [
          {
            "name": "SOME STRING",
            "field": "stringie",
            "type": "string"
          },
          {
            "name": "SOME ID",
            "field": "id",
            "type": "id"
          },
          {
            "name": "SNumbaer",
            "field": "numba",
            "type": "number"
          },
          {
            "name": "EMAILE",
            "field": "emailtje",
            "type": "email"
          }
        ]
      }
    }
  }

